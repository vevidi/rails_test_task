<p id="notice"><%= notice %></p>

<p>
  <strong>Age:</strong>
  <%= @feedback_form.age %>
</p>

<p>
  <strong>Name:</strong>
  <%= @feedback_form.name %>
</p>

<p>
  <strong>Appointment date:</strong>
  <%= @feedback_form.appointment_date %>
</p>

<% if @feedback_form.file_name %>
    <p>
      <strong>File:</strong>
      <%= link_to @feedback_form.file_name, @feedback_form.attachment.url, target: :_blank %>
    </p>
<% end %>

<%= link_to 'Edit', edit_feedback_form_path(@feedback_form) %> |
<%= link_to 'Back', feedback_forms_path %>
